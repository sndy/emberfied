<div id="wrapper">
    <div id="sidebar-wrapper">
        <ul class="sidebar-nav">
            <li class="sidebar-brand">
                <a>Dashboard</a>
            </li>
            <li>
                {{#link-to 'index'}}Home{{/link-to}}
            </li>
            <li>
                <a href="/dashboard/#maps">Maps</a>
            </li>
        </ul>
    </div>
    <div id="page-content-wrapper">
        <div id="maps" class="container-fluid">
            <div class="row">
                <div id="worldPopulationDensity" class="col-lg-12">
                    {{world-map type='worldPopulationDensity' dataSeriesName='Population density' title='Population Density Map'}}
                </div>
            </div>
            <div class="row">
                <div id="worldPopulationBubble" class="col-lg-12">
                    {{world-map type='worldPopulation' dataSeriesName='Population 2013' title='World Population Bubble Map'}}
                </div>
            </div>
            <div class="row">
                <div id="worldMapFixedTooltip" class="col-lg-12">
                    {{world-map type='worldMapFixedTooltip' dataSeriesName='Population density Fixed HTML' title='Population density Fixed HTML'}}
                </div>
            </div>
        </div>
    </div>
</div>